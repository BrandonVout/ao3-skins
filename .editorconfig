# http://editorconfig.org

# top-most EditorConfig file
root = true

# all files
[*]
indent_style = space
indent_size = 2
# tab_width = 2
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
insert_final_newline = true
max_line_length = 120

# markdown files
[*.md]
trim_trailing_whitespace = false
